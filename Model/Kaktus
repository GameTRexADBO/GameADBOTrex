import java.util.*;
public class Kaktus extends Rintangan{
    
    public Kaktus(int x, int y, int z){
        super (x, y, z);
    }
    
    @Override
    protected void spawn{
        Random r = new Random();
        
        //random jarak dan tinggi
        //asumsi ada 2 tinggi, 1 = rendah; 2 = tinggi
        Kaktus k= new Kaktus(r.nextInt(), r.nextInt(2), 0);
    }
}
